const setHeroHeight = () => {
  const hero = document.getElementById('hero');
  const header = document.querySelector('header.site');
  const topbar = document.querySelector('.topbar');

  if (hero && header && topbar) {
    // On ne fait cette correction que sur les √©crans mobiles (largeur < 880px)
    if (window.innerWidth <= 880) {
      const headerHeight = header.offsetHeight;
      const topbarHeight = topbar.offsetHeight;
      const totalHeaderHeight = headerHeight + topbarHeight;

      // On applique la hauteur r√©elle de la fen√™tre moins la hauteur r√©elle du header
      hero.style.minHeight = `calc(${window.innerHeight}px - ${totalHeaderHeight}px)`;
